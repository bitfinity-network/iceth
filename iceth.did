type EthRpcResult = variant {
  Ok: blob;
  Err : opt variant {
    TooFewCycles : text;
    ServiceUrlParseError;
    ServiceUrlHostMissing : text;
    ServiceUrlHostNotAllowed : text;
    HttpRequestError : record { code: nat32; message: text };
  }
};
service : {
  ethRpcRequest : (jsonRpcPayload: text, serviceUrl: text, maxResultSize: nat64) -> (EthRpcResult);
}
